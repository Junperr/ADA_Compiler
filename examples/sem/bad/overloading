with Ada.Text_IO; use Ada.Text_IO;

procedure Record4 is
    function f (x,y,z : Integer) return Integer is
            a : Integer := 8;
            Next  : List;
            procedure Record5(a:Integer) is
                type List is record hi : Integer; end record;
                begin
                    Put('x');
                end Record5;
        begin
            return x + 1;
        end f;
    --f : Integer;
begin
    t := f(1,2,3);
    Put_Line(Integer'Image(t));
end Record4;