with Ada.Text_IO; use Ada.Text_IO;

procedure Pascal is
    a : Integer;
   type Node;
   type Node is record
      Value: Integer;
      Father: Integer;
   end record;

   function F return integer is begin
    if a then return 2;
    elsif 1=2 or else 2=3 or else 3=4 then return 1;
    elsif 4=5 and then 5=6 and then 6=7 then return 2;
    else return 3;
    end if;
    end;
begin a := F; end;